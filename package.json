{
  "name": "inteyvat",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug=0.0.0.0:9229 --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{ts,public,src,apps,libs,test}/**/*.{t,j}s\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./ts/test/jest-e2e.json",
    "test:e2e:debug": "node --inspect-brk=0.0.0.0:9229 node_modules/.bin/jest --config ./ts/test/jest-e2e.json",
    "pretypeorm:migration:run": "nest build",
    "typeorm:migration:run": "typeorm migration:run -d dist/src/pg.data-source.js",
    "precassandra:migration:run": "nest build",
    "cassandra:migration:run": "node dist/src/migrate",
    "prefixtures": "nest build",
    "fixtures": "fixtures load --dataSource=dist/src/pg.data-source.js fixtures",
    "precommand": "nest build",
    "command": "node dist/src/command.js",
    "command_": "node -r tsconfig-paths/register -r ts-node/register ts/src/command.ts"
  },
  "dependencies": {
    "@fastify/static": "^6.6.0",
    "@nestjs/axios": "^1.0.0",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/platform-fastify": "^9.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "@ntegral/nestjs-sentry": "^4.0.0",
    "@sendgrid/mail": "^7.7.0",
    "@sentry/node": "^7.30.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "date-fns": "^2.29.3",
    "lodash": "^4.17.21",
    "nest-commander": "^3.4.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.0",
    "telegraf": "^4.11.2",
    "typeorm": "^0.3.12"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/microservices": "^9.2.1",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.0.0",
    "@types/jest": "28.1.8",
    "@types/lodash": "^4.14.191",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "nock": "^13.3.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.0",
    "typeorm-fixtures-cli": "^3.0.2",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "ts",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
